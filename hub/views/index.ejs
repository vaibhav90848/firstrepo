<body>
    <div class="min vh-100 p-5 bg bg-dark ">
        <form action="/create" method="post">
            <input class="rounded-2 p-2" type="text" placeholder="username" name="username">
            <input class="rounded-2 p-2" type="email" placeholder="email" name="email">
            <input class="rounded-2 p-2" type="password" placeholder="password" name="password">
            <input class="rounded-2 p-2" type="text" placeholder="game" name="game">
            <input class="rounded-2 p-2" type="number" placeholder="height" name="height">
            <input class="rounded-2 p-2" type="number" placeholder="weight" name="weight">
            <input class="rounded-2 p-2 mb-2" type="submit" value="create User">
        </form>
        <a href="/login" class="d-block text-secondary h1">Login!</a>
        <a href="/read" class="d-block text-secondary h1">Watch our users</a>
        <a href="/profile" class="d-block text-secondary h1">profile</a>
        <a href="/post" class="d-block text-secondary h1">Post a message</a>
        <a href="/logout" class="d-block text-secondary h1">Logout</a>
    </div>
